# Автор24
AVTOR24_EMAIL=<email для входа>
AVTOR24_PASSWORD=<пароль от аккаунта>
AVTOR24_BASE_URL=https://avtor24.ru

# OpenAI
OPENAI_API_KEY=<ключ>
OPENAI_MODEL_MAIN=gpt-4o
OPENAI_MODEL_FAST=gpt-4o-mini

# Антиплагиат
ETXT_API_KEY=<ключ ETXT если есть>
TEXTRU_API_KEY=<ключ text.ru>
MIN_UNIQUENESS=50

# Дашборд
DASHBOARD_USERNAME=<логин>
DASHBOARD_PASSWORD_HASH=<bcrypt хеш пароля>
DASHBOARD_SECRET_KEY=<случайная строка для JWT>

# База данных
DATABASE_URL=postgresql+asyncpg://user:pass@host:5432/avtor24bot

# Прокси
PROXY_RU=socks5://user:pass@ip:port

# Настройки бота
MAX_CONCURRENT_ORDERS=5
AUTO_BID=true
BID_COMMENT_TEMPLATE=default
MIN_PRICE_RUB=300
MAX_PRICE_RUB=50000
SCAN_INTERVAL_SECONDS=60
SPEED_LIMIT_MIN_DELAY=30
SPEED_LIMIT_MAX_DELAY=120
